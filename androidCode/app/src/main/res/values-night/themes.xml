<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Dark.Theme.MobieleBeleving" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
        <item name="colorPrimary">@color/action_bar_color</item>
        <item name="android:fontFamily">@font/kalamregular</item>
        <item name="android:windowBackground">@color/black</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHighlight">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:colorButtonNormal">@color/orange</item>
    </style>
    <style name="textView">
        <item name="android:background">@color/purple</item>
    </style>
    <style name="recyclerView">
        <item name="android:background">@color/orange</item>
    </style>
    <style name="questionButton">
        <item name="backgroundTint">@color/purple</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/action_bar_color</item>
        <item name="colorPrimaryDark">@color/action_bar_color</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Theme.MobieleBeleving" parent="Base.Dark.Theme.MobieleBeleving" />
</resources>